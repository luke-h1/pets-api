    "@types/lodash": "^4.17.13",
    "@types/multer": "^1.4.12",
    "@types/multer-s3": "^3.0.3",
    "@types/node": "^20.8.6",
    "@types/papaparse": "^5.3.15",
    "@types/qs": "^6.9.17",
    "@types/superagent": "^8.1.9",
