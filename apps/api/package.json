{
  "name": "@pets/api",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "clean": "rm -rf dist",
    "db:down": "pnpm prisma migrate reset",
    "db:drop": "DATABASE_URL='postgresql://pets:pets@localhost:5555/pets?schema=public' ts-node scripts/drop.ts && DATABASE_URL='postgresql://pets:pets@localhost:5555/pets?schema=public' pnpm prisma db push --force-reset",
    "db:format": "pnpm prisma format",
    "db:generate": "pnpm prisma generate",
    "db:migrate-dev": "pnpm prisma migrate dev",
    "db:migrate-new": "pnpm prisma migrate dev --name <NAME>",
    "db:migrate-prod": "pnpm prisma migrate deploy",
    "db:seed": "ts-node prisma/seed.ts",
    "db:studio": "pnpm prisma studio",
    "dev": "NODE_ENV=development ts-node-dev -r tsconfig-paths/register --respawn --transpile-only src/index.ts",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx,css,scss,html}\"",
    "format:check": "prettier --list-different \"src/**/*.{js,jsx,ts,tsx,css,scss,html}\"",
    "kill:port": "npx kill-port 8000",
    "lint": "eslint --fix --ext .ts,.tsx,.js,.jsx .",
    "start": "node -r ts-node/register/transpile-only -r tsconfig-paths/register -r newrelic dist/apps/api/src/index.js",
    "test": "./scripts/setup-testdb.sh && jest --runInBand",
    "test:ci": "./scripts/setup-testdb.sh && jest --ci --reporters=default --reporters=jest-junit --runInBand",
    "tsc": "tsc --noEmit"
  },
  "dependencies": {
    "@asteasolutions/zod-to-openapi": "^7.1.1",
    "@aws-sdk/client-s3": "^3.685.0",
    "@aws-sdk/lib-storage": "^3.685.0",
    "@pets/validation": "workspace:*",
    "@prisma/client": "5.17.0",
    "@prisma/generator-helper": "5.17.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.20.2",
    "compression": "^1.7.4",
    "connect-redis": "^6.0.0",
    "cors": "^2.8.5",
    "dayjs": "^1.11.13",
    "dotenv": "^16.4.5",
