    "express": "^4.19.2",
    "express-async-errors": "^3.1.1",
    "express-session": "^1.18.0",
    "fs-extra": "^11.2.0",
    "ioredis": "^5.4.1",
    "ioredis-mock": "^8.9.0",
    "lodash": "^4.17.21",
    "multer": "1.4.5-lts.1",
    "multer-s3": "^3.0.1",
    "newrelic": "^12.5.0",
    "papaparse": "^5.4.1",
    "pino": "^8.6.1",
    "pino-pretty": "^9.1.0",
    "prisma": "5.17.0",
    "qs": "^6.13.0",
    "redis": "^4.7.0",
    "swagger-jsdoc": "^6.2.8",
