{
  "name": "pets-api",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "clean": "rm -rf dist",
    "build": "npm run clean && tsc && cp -r src/data dist/data",
    "tsc": "tsc --noEmit",
    "dev": "NODE_ENV=development ts-node-dev -r tsconfig-paths/register --respawn --transpile-only src/index.ts",
    "start": "NODE_ENV=production node dist/index.js",
    "test": "jest",
    "lint": "eslint **/*.ts",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx,css,scss,html}\"",
    "format:check": "prettier --list-different \"src/**/*.{js,jsx,ts,tsx,css,scss,html}\"",
    "kill:port": "npx kill-port 8000",
    "typeorm": "typeorm-ts-node-commonjs",
    "db:migration-revert": "typeorm migration:revert -- -d src/db/app-data-source",
    "db:migration-generate": "pnpm typeorm migration:generate src/db/migrations -d src/db/app-data-source.ts",
    "db:migration-create-empty": "pnpm typeorm migration:create src/db/migrations/init -- -d src/db/app-data-source",
    "db:migration-up": "pnpm typeorm migration:run -- -d src/db/app-data-source",
    "db:migration-show": "typeorm migration:show  -- -d src/db/app-data-source"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "compression": "^1.7.4",
    "connect-redis": "^6.0.0",
    "cors": "^2.8.5",
    "dayjs": "^1.10.7",
    "dotenv": "^16.3.1",
    "express": "^4.19.2",
    "express-async-errors": "^3.1.1",
    "express-session": "^1.18.0",
    "ioredis": "^5.4.1",
    "lodash": "^4.17.21",
    "pg": "^8.12.0",
    "pino": "^8.6.1",
    "pino-pretty": "^9.1.0",
    "redis": "^4.7.0",
    "reflect-metadata": "^0.2.2",
    "typeorm": "^0.3.20",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/compression": "^1.7.5",
    "@types/connect-redis": "^0.0.23",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.20",
    "@types/express-session": "^1.18.0",
    "@types/ioredis-mock": "^8.2.5",
    "@types/jest": "^29.5.12",
    "@types/lodash": "^4.17.6",
    "@types/node": "^20.8.6",
    "@types/supertest": "^6.0.2",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^7.14.1",
    "@typescript-eslint/parser": "^7.14.1",
    "babel-jest": "^29.7.0",
    "concurrently": "^8.2.1",
    "eslint": "^8.52.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-next": "^14.2.4",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "ioredis-mock": "^8.9.0",
    "jest": "^29.7.0",
    "nodemon": "^3.1.4",
    "prettier": "^3.3.3",
    "supertest": "^7.0.0",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.2.2",
    "uuid": "^10.0.0"
  }
}
