{
  "name": "pets-api",
  "version": "1.0.0",
  "main": "index.js",
  "packageManager": "pnpm@9.0.6",
  "engines": {
    "pnpm": "9.0.6"
  },
  "scripts": {
    "alphabetize": "node scripts/alphabetize.js",
    "build": "tsc",
    "clean": "rm -rf dist",
    "commit": "npx cz",
    "e2e:local": "make e2e-local",
    "db:seed": "ts-node prisma/seed.ts",
    "db:down": "pnpm prisma migrate reset",
    "start:prod": "NODE_ENV=production node dist/src/index.js",
    "db:drop": "pnpm prisma db push --force-reset",
    "db:format": "pnpm prisma format",
    "db:generate": "pnpm prisma generate",
    "db:migrate-new": "pnpm prisma migrate dev --name <NAME>",
    "db:studio": "pnpm prisma studio",
    "db:migrate-dev": "pnpm prisma migrate dev",
    "db:migrate-prod": "pnpm prisma migrate deploy",
    "dev": "NODE_ENV=development ts-node-dev -r tsconfig-paths/register --respawn --transpile-only src/index.ts",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx,css,scss,html}\"",
    "format:check": "prettier --list-different \"src/**/*.{js,jsx,ts,tsx,css,scss,html}\"",
    "kill:port": "npx kill-port 8000",
    "lint": "eslint --fix --ext .ts,.tsx,.js,.jsx .",
    "start": "NODE_ENV=production node dist/index.js",
    "test": "./scripts/setup-testdb.sh && jest --runInBand",
    "test:ci": "./scripts/setup-testdb.sh && jest --ci --reporters=default --reporters=jest-junit --runInBand",
    "tsc": "tsc --noEmit"
  },
  "dependencies": {
    "@asteasolutions/zod-to-openapi": "^7.1.1",
    "@prisma/client": "^5.17.0",
    "@prisma/generator-helper": "^5.17.0",
    "prisma": "^5.17.0",
    "argon2": "^0.40.3",
    "bcryptjs": "^2.4.3",
    "better-sqlite3": "^11.1.2",
    "body-parser": "^1.20.2",
    "compression": "^1.7.4",
    "connect-redis": "^6.0.0",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "dotenv-cli": "^7.4.2",
    "dayjs": "^1.10.7",
    "express": "^4.19.2",
    "express-async-errors": "^3.1.1",
    "express-session": "^1.18.0",
    "ioredis": "^5.4.1",
    "ioredis-mock": "^8.9.0",
    "lodash": "^4.17.21",
    "pg": "^8.12.0",
    "pino": "^8.6.1",
    "pino-pretty": "^9.1.0",
    "qs": "^6.13.0",
    "redis": "^4.7.0",
    "reflect-metadata": "^0.2.2",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "xml": "^1.0.1",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.3.0",
    "@commitlint/config-conventional": "^19.2.2",
    "@faker-js/faker": "^8.4.1",
    "@playwright/test": "^1.45.1",
    "@types/bcryptjs": "^2.4.6",
    "@types/body-parser": "^1.19.5",
    "@types/compression": "^1.7.5",
    "@types/connect-redis": "^0.0.23",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.20",
    "@types/express-session": "^1.18.0",
    "@types/ioredis-mock": "^8.2.5",
    "@types/jest": "^29.5.12",
    "@types/lodash": "^4.17.6",
    "@types/node": "^20.8.6",
    "@types/qs": "^6.9.15",
    "@types/superagent": "^8.1.8",
    "@types/supertest": "^6.0.2",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.6",
    "@types/uuid": "^10.0.0",
    "@types/xml": "^1.0.11",
    "@typescript-eslint/eslint-plugin": "^7.14.1",
    "@typescript-eslint/parser": "^7.14.1",
    "babel-jest": "^29.7.0",
    "commitizen": "4.3.0",
    "concurrently": "^8.2.1",
    "cookiejar": "^2.1.4",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.52.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-next": "^14.2.4",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "git-cz": "^4.9.0",
    "husky": "^9.0.11",
    "jest": "^29.7.0",
    "jest-junit": "^16.0.0",
    "lint-staged": "15.2.7",
    "prettier": "^3.3.3",
    "superagent": "^9.0.2",
    "supertest": "^7.0.0",
    "supertest-session": "^5.0.1",
    "testcontainers": "^10.11.0",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.2.2",
    "uuid": "^10.0.0"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,css,scss,html}": [
      "prettier --write"
    ],
    "*.{ts,tsx,js,jsx}": [
      "eslint"
    ],
    "package.json": [
      "node scripts/alphabetizePackageJson.js"
    ]
  },
  "husky": {
    "hooks": {
      "prepare-commit-msg": "exec < /dev/tty && npx cz --hook || true"
    }
  }
}
